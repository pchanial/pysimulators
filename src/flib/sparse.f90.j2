    subroutine fsr_rot2d_matvec_i{{ combo.isize }}_r{{ combo.rsize }}_v{{ combo.vsize }}(matrix, input,      &
                                         output, ncolmax, ninput, noutput)
        integer, parameter           :: s = {{ combo.isize }} + 2 * {{ combo.rsize }}
        integer(int64), intent(in)   :: ncolmax
        integer(int64), intent(in)   :: ninput
        !f2py integer(int64), intent(in), optional, depend(input) :: ninput=size(input)/2
        integer(int64), intent(in)   :: noutput
        !f2py integer(int64), intent(in), optional, depend(output) :: noutput=size(output)/2
        !f2py integer*1, intent(in)  :: matrix(ncolmax*s*noutput)
        type(PointingElementRot2d_i{{ combo.isize }}_r{{ combo.rsize }}), intent(in) ::            &
            matrix(ncolmax, noutput)
        !f2py real({{ combo.vkind }})          :: input(2*ninput)
        real({{ combo.vkind }}), intent(in)    :: input(2,0:ninput-1)
        !f2py real({{ combo.vkind }})          :: output(2*noutput)
        real({{ combo.vkind }}), intent(inout) :: output(2,noutput)

        integer(int64) :: i, j
        integer({{ combo.ikind }}) :: col

        !$omp parallel do private(col)
        do i = 1, noutput
            do j = 1, ncolmax
                col = matrix(j,i)%index
                if (col < 0) cycle
                output(1,i) = output(1,i) + matrix(j,i)%r11 * input(1,col) -   &
                                            matrix(j,i)%r21 * input(2,col)
                output(2,i) = output(2,i) + matrix(j,i)%r21 * input(1,col) +   &
                                            matrix(j,i)%r11 * input(2,col)
            end do
        end do
        !$omp end parallel do

    end subroutine


