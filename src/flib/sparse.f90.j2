    subroutine fsc_matvec_i{{ combo.isize }}_r{{ combo.rsize }}_v{{ combo.vsize }}(matrix, input, output,    &
                                   nrowmax, ninput, noutput)
        integer, parameter           :: s = {{ combo.isize }} + {{ combo.rsize }}
        integer(int64), intent(in)   :: nrowmax
        integer(int64), intent(in)   :: ninput
        !f2py integer(int64), intent(in), optional, depend(input) :: ninput=len(input)
        integer(int64), intent(in)   :: noutput
        !f2py integer(int64), intent(in), optional, depend(output) :: noutput=len(output)
        !f2py integer*1, intent(in)  :: matrix(nrowmax*s*ninput)
        type(PointingElement_i{{ combo.isize }}_r{{ combo.rsize }}), intent(in) ::                 &
            matrix(nrowmax, ninput)
        real({{ combo.vkind }}), intent(in)    :: input(ninput)
        real({{ combo.vkind }}), intent(inout) :: output(0:noutput-1)

        integer(int64) :: i, j
        integer({{ combo.ikind }}) :: col

        if (nrowmax == 1) then
            !$omp parallel do private(col)
            do i = 1, ninput
                col = matrix(1,i)%index
                if (col < 0) cycle
                !$omp atomic
                output(col) = output(col) + matrix(1,i)%value * input(i)
            end do
            !$omp end parallel do
            return
        end if

        !$omp parallel do private(col)
        do i = 1, ninput
            do j = 1, nrowmax
                col = matrix(j,i)%index
                if (col < 0) cycle
                !$omp atomic
                output(col) = output(col) + matrix(j,i)%value * input(i)
            end do
        end do
        !$omp end parallel do

    end subroutine


