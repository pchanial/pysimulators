        integer, parameter          :: s = {{ combo.isize }} + 3 * {{ combo.rsize }}
        integer(int64), intent(in)  :: ninput
        !f2py integer(int64), intent(in), optional, depend(input) :: ninput=size(input)/3
        integer(int64), intent(in)  :: noutput
        !f2py integer(int64), intent(in), optional, depend(matrix) :: noutput=len(matrix)/{{ combo.isize|int + 3 * combo.rsize|int }}
        !f2py integer*1, intent(in) :: matrix(s*noutput)
        type(PointingElementRot3d_i{{ combo.isize }}_r{{ combo.rsize }}), intent(in) :: matrix(noutput)
        !f2py real({{ combo.vkind }})          :: input(3*ninput)
        real({{ combo.vkind }}), intent(inout) :: input(3,0:ninput-1)

        integer(int64)   :: i
        integer({{ combo.ikind }}) :: col
        real({{ combo.vkind }})    :: val

        !$omp parallel do private(col, val)
        do i = 1, noutput
            col = matrix(i)%index
            if (col < 0) cycle
            val = matrix(i)%r22**2 + matrix(i)%r32**2
            !$omp critical
            input(1,col) = input(1,col) + matrix(i)%r11**2
            input(2,col) = input(2,col) + val
            input(3,col) = input(3,col) + val
            !$omp end critical
        end do
        !$omp end parallel do

    end subroutine

{% endif %}
{% endfor %}

{% for combo in combinations %}
{% if combo.ikind in ['int8', 'int16', 'int32', 'int64'] and loop.index0 >= 12 %}

    subroutine fsr_kernel_i{{ combo.isize }}(matrix, kernel, ncolmax, m, n, stride)
        integer(int64), intent(in)        :: ncolmax
        integer(int64), intent(in)        :: m
        integer(int64), intent(in)        :: n
        integer(int64), intent(in)        :: stride
        integer(int8), intent(in), target :: matrix(0:ncolmax*m*stride-1)
        logical(1), intent(inout)         :: kernel(0:n-1)
        integer({{ combo.ikind }}), pointer :: k
        integer(int32), pointer :: val(:)
        integer(int64) :: i, j, pos

        !$omp parallel do private(k, pos)
        do i = 1, m
            do j = 1, ncolmax
                pos = ((i - 1) * ncolmax + j - 1) * stride
                call c_f_pointer(c_loc(matrix(pos)), k)
                if (k < 0) cycle
                call c_f_pointer(c_loc(matrix(pos + {{ combo.isize }})), val,            &
                                 [(stride - {{ combo.isize }}) / 4])
                if (any(val /= 0)) kernel(k) = .false.
            end do
        end do
        !$omp end parallel do

    end subroutine

    subroutine fsr_restrict_i{{ combo.isize }}(matrix, mask, ncolmax, m, n, stride, ncol)
        integer(int64), intent(in)           :: ncolmax
        integer(int64), intent(in)           :: m
        integer(int64), intent(in)           :: n
        integer(int64), intent(in)           :: stride
        integer(int8), intent(inout), target :: matrix(0:ncolmax*m*stride-1)
        logical(1), intent(in)               :: mask(0:n-1)
        integer({{ combo.ikind }}), intent(out)        :: ncol

        integer({{ combo.ikind }}), pointer :: col
        integer(int64) :: i, j, pos
        integer({{ combo.ikind }}) :: new_index(0:n-1)

        ncol = 0
        do i = 0, n-1
            if (mask(i)) then
                new_index(i) = ncol
                ncol = ncol + 1_{{ combo.ikind }}
            else
                new_index(i) = -1_{{ combo.ikind }}
            end if
        end do

        !$omp parallel do private(col, pos)
        do i = 1, m
            do j = 1, ncolmax
                pos = ((i - 1) * ncolmax + j - 1) * stride
                call c_f_pointer(c_loc(matrix(pos)), col)
                if (col < 0) cycle
                col = new_index(col)
            end do
        end do
        !$omp end parallel do


    end subroutine

{% endif %}
{% endfor %}


